<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout"	xmlns:core="sap.ui.core" id="driverLogoutFragment">
	<Dialog title="{i18n>ENTER_YOUR_NUMBER}">
		<l:VerticalLayout class="fragment sapUiContentPadding" width="100%">
			<l:content>
				<Input value="{oModel>/driverphnum}"/>
				<HBox class="Submit">
					<Button text="{i18n>SUBMIT}" press="onSubmitDriverLogout"></Button>
				</HBox>
			</l:content>
		</l:VerticalLayout>
	</Dialog>
</core:FragmentDefinition>